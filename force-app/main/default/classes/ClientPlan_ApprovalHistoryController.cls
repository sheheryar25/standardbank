public without sharing class ClientPlan_ApprovalHistoryController {

    @AuraEnabled
    public static List<ProcessInstance> getApprovalHistory() {
        List<ProcessInstance> CPApprovalHistList =  [SELECT CreatedDate,Status , SubmittedById FROM ProcessInstance where ProcessInstance.ProcessDefinition.DeveloperName ='Client_Plan_Approval_v1_2'];
        return CPApprovalHistList;
    }
}