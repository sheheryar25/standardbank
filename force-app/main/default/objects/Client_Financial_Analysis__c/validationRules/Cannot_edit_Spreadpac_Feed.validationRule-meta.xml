<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_edit_Spreadpac_Feed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(RecordType.DeveloperName  = &apos;Spreadpac_Feed&apos;,
AND($Profile.Name &lt;&gt; &quot;Business Administrator&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;Api User&quot; ))</errorConditionFormula>
    <errorMessage>Cannot edit public financial analysis published by Spreadpac</errorMessage>
</ValidationRule>
