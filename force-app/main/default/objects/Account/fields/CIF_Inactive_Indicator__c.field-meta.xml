<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CIF_Inactive_Indicator__c</fullName>
    <description>Displays an Red Visual Indicator for Inactive CIF Clients.</description>
    <externalId>false</externalId>
    <formula>Image(
Case(CIF_Client_Status__c,
&quot;Closed&quot;,&quot;/img/samples/flag_red.gif&quot;,
&quot;Open&quot;,&quot;/img/samples/flag_green.gif&quot;,
&quot;/img/samples/flag_yellow.gif&quot;),
CIF_Client_Status__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>CIF Active Indicator</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
