<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Estimated_Share_of_Wallet__c</fullName>
    <description>Estimated Standard Bank Revenue / Estimated Total Wallet Size</description>
    <externalId>false</externalId>
    <formula>If((OR(Nullvalue(Estimated_Standard_Bank_Revenue__c ,0)==0, Nullvalue(Estimated_Total_Wallet_Size__c,0)==0)), 
0, Estimated_Standard_Bank_Revenue__c  /  Estimated_Total_Wallet_Size__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Estimated Standard Bank Revenue / Estimated Total Wallet Size</inlineHelpText>
    <label>Estimated Share of Wallet (%)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
