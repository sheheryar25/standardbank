<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Financial_year_end__c</fullName>
    <description>SFP-5354 How many days between now and the Financial year end.</description>
    <externalId>false</externalId>
    <formula>DATE(YEAR(TODAY()),12,31) -  TODAY()</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculation to display how many days between today and the end of the year</inlineHelpText>
    <label>Financial year end</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
