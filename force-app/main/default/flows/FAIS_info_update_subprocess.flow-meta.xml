    <description>US-2217 Create task if FAIS Review Date&apos; on the client is not blank and today&apos;s date is greater than or equal to FAIS Reminder Date</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>$Label.FAIS_reminder_subject </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_10_myRule_9_A1_3053679029</name>
        <dataType>String</dataType>
        <expression>{!$Label.FAIS_reminder_subject}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>IF( ISNULL([Account].FAIS_Review_Date__c)   ,TODAY() + 7, [Account].FAIS_Review_Date__c +365)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_11_myRule_9_A1_7110887414</name>
        <dataType>Date</dataType>
        <expression>IF( ISNULL({!SObject.FAIS_Review_Date__c})   ,TODAY() + 7, {!SObject.FAIS_Review_Date__c} +365)</expression>
