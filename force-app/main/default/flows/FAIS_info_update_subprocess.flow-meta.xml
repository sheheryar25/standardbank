    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>&quot;This is a reminder to update the FAIS information of &quot;+ [Account].Name  + &quot;.&quot; + 
&quot; This information is required by regulatory standards to be updated annually or whenever there is a material change. Please update the relevant information on the regulatory tab of this client.&quot; +  

&quot; Once this information has been updated this task will automatically be closed on your behalf.&quot; </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_6_myRule_4_A1_1944632137</name>
        <dataType>String</dataType>
        <expression>&quot;This is a reminder to update the FAIS information of &quot;+ {!SObject.Name}  + &quot;.&quot; + 
&quot; This information is required by regulatory standards to be updated annually or whenever there is a material change. Please update the relevant information on the regulatory tab of this client.&quot; +  

&quot; Once this information has been updated this task will automatically be closed on your behalf.&quot;</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>$Label.FAIS_reminder_subject+ &apos;  &apos;  + [Account].Name </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_7_myRule_4_A1_9762582599</name>
        <dataType>String</dataType>
        <expression>{!$Label.FAIS_reminder_subject}+ &apos;  &apos;  + {!SObject.Name}</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
OR(
ISPICKVAL([Account].FAIS_reminder__c, &apos;HAS_REMINDER_NEED&apos;) ,
ISPICKVAL([Account].FAIS_reminder__c, &apos;NEED_REMINDER&apos;) 
),
(TODAY() - [Account].FAIS_Review_Date__c &lt; 365 - 8)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
OR(
ISPICKVAL({!SObject.FAIS_reminder__c}, &apos;HAS_REMINDER_NEED&apos;) ,
ISPICKVAL({!SObject.FAIS_reminder__c}, &apos;NEED_REMINDER&apos;) 
),
(TODAY() - {!SObject.FAIS_Review_Date__c} &lt; 365 - 8)
