    <formulas>
        <description>Updates the case&apos;s subject to include the case origin and service type that has been selected</description>
        <name>SVC_VAR_CaseSubject</name>
        <dataType>String</dataType>
        <expression>TEXT({!SVC_VAR_CaseSource}) &amp;  &quot; - &quot; &amp; {!SVC_CMP_Subject}</expression>
    </formulas>
    <formulas>
        <name>SVC_VAR_CaseSubjectFromCTI</name>
        <dataType>String</dataType>
        <expression>{!SVC_Origin} &amp;  &quot; - &quot; &amp; {!SVC_CMP_Subject}</expression>
    </formulas>
    <formulas>
        <description>Ensures the root cause long text area only takes in 1000 characters</description>
        <name>SVC_VAR_TrimMyRootCause</name>
        <dataType>String</dataType>
        <expression>TRIM(LEFT({!SVC_CMP_RootCause}, 1000))</expression>
    </formulas>
    <formulas>
        <description>Ensures the root cause resolution long text area only takes in 1000 characters</description>
        <name>SVC_VAR_TrimRootCauseResolution</name>
        <dataType>String</dataType>
        <expression>TRIM(LEFT({!SVC_CMP_RootCauseResolution}, 1000))</expression>
    </formulas>
    <formulas>
        <description>Stores the user&apos;s country</description>
        <name>SVC_VAR_UserCountry</name>
        <dataType>String</dataType>
        <expression>{!$User.Country}</expression>
    </formulas>
