    <decisions>
        <description>Decision to confirm if the case was created from the CTI adapter, if yes, then we always pass the Case Source value as Client Phone to ensure we override any value that users may have selected other than Phone</description>
        <name>SVC_CaseCreatedFromCTIAdapter</name>
        <label>Case Created From CTI Adapter</label>
        <locationX>314</locationX>
        <locationY>1514</locationY>
        <defaultConnector>
            <targetReference>SVC_SCREEN_NavigateToCaseRecord</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>SVC_CaseCreatedFromCTI</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SVC_AmazonConnectContactId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SVC_Origin</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SVC_UpdateCaseSubjectAndSource</targetReference>
            </connector>
            <label>Case Created From CTI</label>
        </rules>
    </decisions>
